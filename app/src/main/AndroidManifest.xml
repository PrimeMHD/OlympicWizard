<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chizaitongji.example.com.chizaitongji">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/olympic_appico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/olympic_appico"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.AddGameActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        />
        <activity
            android:name=".Activity.AddNationActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".Activity.SetCompActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog"/>
    </application>

</manifest>